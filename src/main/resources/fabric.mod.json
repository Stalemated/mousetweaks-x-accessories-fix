		"main": [
			"com.stalemated.MouseTweaksXAccessoriesFix"
		],
		"client": [
			"com.stalemated.MouseTweaksXAccessoriesFixClient"
		]
	},
	"mixins": [
		"mousetweaks-x-accessories-fix.mixins.json",
