	"authors": [
		"Stalemated"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mousetweaks-x-accessories-fix/icon.png",
	"environment": "*",
